{
  "name": "raven-bank-ui",
  "version": "0.6.12",
  "description": "A Batteries included React Component Library by Raven bank.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mona-chen/raven-ui"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn build:webpack",
    "build:esm": "tsc",
    "build:webpack": "webpack --mode=production",
    "build:rollup": "./node_modules/.bin/rollup -c ",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "dependencies": {
    "react-countdown": "^2.3.5",
    "react-drag-drop-files": "^2.3.9",
    "react-flatpickr": "^3.10.13",
    "react-live": "^3.1.1",
    "react-loader-spinner": "^5.3.4",
    "react-loading-skeleton": "^3.1.1",
    "react-number-format": "^5.1.3",
    "react-phone-input-2": "^2.15.1",
    "react-pin-field": "^3.1.3",
    "react-select": "^5.7.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "components",
    "css",
    "raven bank"
  ],
  "license": "ISC",
  "author": "Raven bank",
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-flatpickr": "^3.8.8",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "jest": "^29.4.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.4.3",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
